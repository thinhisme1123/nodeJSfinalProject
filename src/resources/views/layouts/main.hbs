<style>

        #chart div {
            margin: 10px;
            background-color: rgb(255, 81, 0);
        }  
        #preloader {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(22, 83, 197, 0.8);
            display: flex;
            justify-content: center;
            align-items: center;
            z-index: 9999;
        }

        #loader {
            border: 8px solid #f3f3f3;
            border-top: 8px solid rgb(255, 51, 0);
            border-radius: 50%;
            width: 50px;
            height: 50px;
            animation: spin 1s linear infinite;
        }

    @keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
    }

</style>

<body>
   
    <div class="">
          <div id="preloader">
                <div id="loader"></div>
            </div>
        {{#if userLogin}}
            <div class="header">
                {{> header}}
            </div>
            <div class="nav">
                {{> nav}}
            </div>
            {{{body}}}
            <div class="footer">
                {{> footer}}
            </div>
        {{else}}
            {{{body}}}
        {{/if}}
    </div>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    {{!-- <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
